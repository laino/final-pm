    "command-line-usage": "4.1.0",
    "deep-equal": "",
    "humanize-duration": "",
    "read-package-config": "",
    "strip-ansi": "",
    "lockfile": "",
    "table": "",
    "stable": "",
    "semver": ""
  },
  "devDependencies": {
    "ansi-to-html": "",
    "nyc": "",
    "coveralls": "",
    "eslint": "",
    "eslint-plugin-mocha": "",
    "turndown": "",
    "mocha": "",
    "chai": "",
    "chai-as-promised": "",
    "tmp": "",
    "rmdir": ""
  }
